{% extends 'admin/adminbase.html.twig' %}
{% block body %}
    <h1>{{ title }}</h1>
    <a href="{{ path('admin_user_create') }}">Створити користувача</a>
    <h2>Список користувачів:</h2>
    <ul>
        {% for user in users %}
            <li>{{ user.email }}, {{ user.roles | json_encode }}</li>
        {% endfor %}
    </ul>
{% endblock %}